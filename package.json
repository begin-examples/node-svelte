{
  "name": "begin-app",
  "version": "0.0.0",
  "description": "Begin basic Svelte app + API",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "lint": "eslint src --fix",
    "start": "npx sandbox"
  },
  "arc": {
    "app": "svelte-app",
    "http": [
      {
        "/api": {
          "method": "get",
          "src": "/api"
        }
      }
    ],
    "tables": [
      {
        "data": {
          "scopeID": "*String",
          "dataID": "**String",
          "ttl": "TTL"
        }
      }
    ]
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "eslint": "^7.19.0",
    "eslint-plugin-svelte3": "",
    "rollup": "^2.40.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.35.0"
  },
  "dependencies": {
    "@architect/sandbox": "^3.3.7",
    "@begin/data": "^1.2.3"
  }
}
